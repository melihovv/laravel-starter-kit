{
    "name": "melihovv/laravel-starter-kit",
    "description": "Laravel starter kit",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminated/console-mutex": "^1.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "melihovv/laravel-env-validator": "^1.0",
        "melihovv/laravel-log-viewer": "1.2.*",
        "spatie/laravel-backup": "^4.19"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "~1.4",
        "garygreen/pretty-routes": "^0.0.6",
        "melihovv/laravel-package-generator": "^0.0.1",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "^2.4",
        "phpunit/phpunit": "~5.7",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^2.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php -r \"touch('.tinker');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan post-update-cmd",
            "php artisan optimize"
        ],
        "phpunit": "phpunit",
        "phpmd": "phpmd app/ text ./phpmd-rules.xml",
        "phpcs": "phpcs --standard=./phpcs-rules.xml --encoding=utf-8 app/",
        "phpcbf": "phpcbf --standard=./phpcs-rules.xml --encoding=utf-8 app/",
        "phpcpd": "phpcpd app/"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
