{
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "illuminated/console-mutex": "^1.1",
    "rap2hpoutre/laravel-log-viewer": "^0.7.0",
    "serafim/properties": "^1.0",
    "barryvdh/laravel-debugbar": "^2.3"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "barryvdh/laravel-ide-helper": "^2.2",
    "itsgoingd/clockwork": "^1.12",
    "garygreen/pretty-routes": "^0.0.1",
    "doctrine/dbal": "^2.5",
    "phpmd/phpmd": "^2.4",
    "squizlabs/php_codesniffer": "^2.7",
    "reliese/laravel": "^0.0.6",
    "thedevsaddam/laravel-schema": "^2.0",
    "mathiasgrimm/laravel-env-validator": "dev-configurable-validation"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/davidstoker/laravel-env-validator"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/DatabaseTestCase.php",
      "tests/TestHelpers.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "npm install"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan post-update-cmd",
      "php artisan optimize"
    ]
  }
}
